	@BehaviorNext?reset@
	@loop@
	<TR>
	@BehaviorNext@<TD WIDTH=25%><a href="javascript:openUp('@BehaviorID@');"><B>@BehaviorID@</B></a></TD>
	@BehaviorNext?EMPTYOK@<TD WIDTH=25%><a href="javascript:openUp('@BehaviorID@');"><B>@BehaviorID@</B></a></TD>
	@BehaviorNext?EMPTYOK@<TD WIDTH=25%><a href="javascript:openUp('@BehaviorID@');"><B>@BehaviorID@</B></a></TD>
	@BehaviorNext?EMPTYOK@<TD WIDTH=25%><a href="javascript:openUp('@BehaviorID@');"><B>@BehaviorID@</B></a></TD>
	</TR>
	@back@
	</TABLE>
